    <item>
        <shape>
            <solid android:color="#000" />
        </shape>
    </item>
    <item android:bottom="500dp" android:left="-200dp" android:right="200dp" android:top="-200dp" android:gravity="start">
        <shape  android:shape="oval" android:innerRadius="200dp">
            <size android:height="30dp"/>
            <solid android:color="#101010"/>

        </shape>
    </item>
    <item android:bottom="-400dp" android:left="0dp" android:right="-300dp" android:top="400dp" android:gravity="start">
        <shape  android:shape="oval" android:innerRadius="200dp">
            <size android:height="30dp"/>
            <solid android:color="#101010"/>

        </shape>
